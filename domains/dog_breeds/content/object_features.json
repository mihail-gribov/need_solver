  "size_group": {
    "id": "size",
    "name": "Size",
    "description": "Adult dog size category based on weight",
    "type": "onehot_soft",
    "source_field": "parameters.weight",
    "unit": "kg",
    "values": [
      {"id": "size_toy", "name": "Toy", "min": 0, "max": 5},
      {"id": "size_small", "name": "Small", "min": 5, "max": 10},
      {"id": "size_medium", "name": "Medium", "min": 10, "max": 25},
      {"id": "size_large", "name": "Large", "min": 25, "max": 45},
      {"id": "size_giant", "name": "Giant", "min": 45, "max": null}
    ],
    "derived": [
      {"id": "size_small_or_less", "name": "Small or less", "formula": "size_toy | size_small"},
      {"id": "size_medium_or_less", "name": "Medium or less", "formula": "size_toy | size_small | size_medium"},
      {"id": "size_large_or_more", "name": "Large or more", "formula": "size_large | size_giant"}
    ]
  },
  "height_group": {
    "id": "height",
    "name": "Height",
    "description": "Adult dog height category at withers",
    "type": "onehot_soft",
    "source_field": "parameters.height",
    "unit": "cm",
    "values": [
      {"id": "height_tiny", "name": "Tiny", "min": 0, "max": 25},
      {"id": "height_small", "name": "Small", "min": 25, "max": 40},
      {"id": "height_medium", "name": "Medium", "min": 40, "max": 55},
      {"id": "height_large", "name": "Large", "min": 55, "max": 70},
      {"id": "height_giant", "name": "Giant", "min": 70, "max": null}
    ],
    "derived": [
      {"id": "height_small_or_less", "name": "Small or less", "formula": "height_tiny | height_small"},
      {"id": "height_medium_or_less", "name": "Medium or less", "formula": "height_tiny | height_small | height_medium"},
      {"id": "height_large_or_more", "name": "Large or more", "formula": "height_large | height_giant"}
    ]
  },
  "lifespan_group": {
    "id": "lifespan",
    "name": "Lifespan",
    "description": "Expected lifespan category",
    "type": "onehot_soft",
    "source_field": "parameters.lifespan",
    "unit": "years",
    "values": [
      {"id": "lifespan_short", "name": "Short", "min": 0, "max": 9},
      {"id": "lifespan_medium", "name": "Medium", "min": 9, "max": 12},
      {"id": "lifespan_long", "name": "Long", "min": 12, "max": 15},
      {"id": "lifespan_very_long", "name": "Very Long", "min": 15, "max": null}
    ],
    "derived": [
      {"id": "lifespan_medium_or_more", "name": "Medium or longer", "formula": "lifespan_medium | lifespan_long | lifespan_very_long"},
      {"id": "lifespan_long_or_more", "name": "Long-lived", "formula": "lifespan_long | lifespan_very_long"}
    ]
  }
}
