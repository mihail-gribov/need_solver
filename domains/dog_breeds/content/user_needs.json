{
  "description": "User needs/axes - the user space basis for matching preferences to breeds",
  "note": "Each axis represented as vector (t, f) where t=true intensity, f=false intensity",
  "axis_types": ["desire", "prohibition", "contextual_fact", "environment_constraint"],
  "formula_syntax": {
    "operators": {
      "&": "AND (conjunction) - min(x1, x2, ...)",
      "|": "OR (disjunction) - max(x1, x2, ...)",
      "~": "NOT (negation) - 1 - x"
    },
    "format": "CNF - Conjunctive Normal Form: clause1 & clause2 & ... where clause = literal | (lit1 | lit2 | ...)",
    "variables": "Object feature identifiers from object_features.json"
  },
